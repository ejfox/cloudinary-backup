{
  "$schema": "https://schema.tauri.app/capability/1",
  "identifier": "default",
  "description": "Default capabilities for Cloudinary Backup",
  "context": "default",
  "permissions": [
    "dialog:allow-open",
    "dialog:allow-save",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-create",
    "fs:allow-remove",
    "fs:allow-rename",
    "fs:allow-copy-file",
    "fs:allow-exists",
    "fs:allow-read-dir"
  ]
}
